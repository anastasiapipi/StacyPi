    "#define full circuit \n",
    "def grovers_algorithm_2(oracle,n):\n",
    "    circuit = QuantumCircuit(n,n)\n",
    "\n",
    "    #first apply Hadamard on both gates\n",
    "    for i in range(n):\n",
    "        circuit.h(i)\n",
    "\n",
    "\n",
    "    #append oracle to circuit\n",
    "    circuit.append(oracle,range(n))\n",
    "\n",
    "    #apply diffuser\n",
    "    circuit.h([0,1])\n",
    "    circuit.z([0,1])\n",
    "    circuit.cz(0,1)\n",
    "    circuit.h([0,1])\n",
    "\n",
    "    #measure\n",
    "    for i in range(n):\n",
    "        circuit.measure(i,i)\n",
    "\n",
    "    return circuit"
   ]
  },

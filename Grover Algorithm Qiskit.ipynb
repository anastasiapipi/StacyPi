    "def diffuser(n):\n",
    "    c = QuantumCircuit(n)\n",
    "    for i in range(n):\n",
    "        c.x(i)\n",
    "        \n",
    "    c += MCMT('z', n-1, 1)\n",
    "    \n",
    "    for i in range(n):\n",
    "        c.x(i)\n",
    "    return c"
   ]
  },
